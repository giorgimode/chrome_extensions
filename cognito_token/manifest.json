{
  "name": "Cognito Token",
  "version": "1.0.0",
  "description": "Get cognito token from local storage",
  "manifest_version": 3,
  "author": "Giorgi Modebadze",
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "nativeMessaging"
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_popup": "index.html",
    "default_title": "Cognito token"
  }
}